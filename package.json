{
	"name": "open-a-port-in-vscode",
	"displayName": "Open a Port in VSCode",
	"description": "An extension that lets you open a certain port in your localhost from inside vscode.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:open-a-port-in-vscode.open"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "open-a-port-in-vscode.open",
				"title": "Open a port inside vscode"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"node-fetch": "^2.6.1"
	}
}
